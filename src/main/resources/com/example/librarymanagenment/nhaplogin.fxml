<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="login-form" stylesheets="@design.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.librarymanagenment.FXMLController">
   <children>
      <ImageView fitHeight="163.0" fitWidth="172.0" layoutX="219.0" layoutY="37.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@login/login1.png" />
         </image>
      </ImageView>
      <AnchorPane layoutX="157.0" layoutY="209.0" prefHeight="123.0" prefWidth="287.0" styleClass="form1" stylesheets="@design.css">
         <children>
            <TextField fx:id="login_id" layoutX="69.0" layoutY="23.0" promptText="MSSV" styleClass="textfield" stylesheets="@design.css" />
            <PasswordField fx:id="pass" layoutX="69.0" layoutY="62.0" promptText="Password" styleClass="textfield" stylesheets="@design.css" />
         </children>
      </AnchorPane>
      <Button fx:id="button_login" layoutX="259.0" layoutY="341.0" mnemonicParsing="false" onAction="#login" styleClass="button2" stylesheets="@design.css" text="Login" textFill="WHITE">
         <font>
            <Font name="System Bold" size="21.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
